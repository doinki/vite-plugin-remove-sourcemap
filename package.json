{
  "name": "vite-plugin-remove-sourcemap",
  "version": "0.2.0",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "release": "pnpm run build && changeset publish"
  },
  "dependencies": {
    "glob": "^11.0.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.3",
    "@eslint/compat": "^1.2.9",
    "@types/node": "^22.15.17",
    "eslint": "^9.26.0",
    "eslint-config-mado": "^0.9.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "author": {
    "email": "doinki94@gmail.com",
    "name": "doinki",
    "url": "https://github.com/doinki"
  },
  "bugs": "https://github.com/doinki/vite-plugin-remove-sourcemap/issues",
  "description": "A Vite plugin that removes sourcemaps from the final build output.",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/doinki/vite-plugin-remove-sourcemap#readme",
  "keywords": [
    "vite",
    "sourcemap",
    "remove"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doinki/vite-plugin-remove-sourcemap.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
